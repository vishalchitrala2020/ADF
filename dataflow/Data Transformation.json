{
	"name": "Data Transformation",
	"properties": {
		"type": "MappingDataFlow",
		"typeProperties": {
			"sources": [
				{
					"dataset": {
						"referenceName": "ds_silver_source",
						"type": "DatasetReference"
					},
					"name": "DimAirline"
				},
				{
					"dataset": {
						"referenceName": "ds_dim_flight",
						"type": "DatasetReference"
					},
					"name": "Dimflight"
				},
				{
					"dataset": {
